{
  "name": "lune/template",
  "description": "PHP templates",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Willem Wollebrants",
      "email": "willem@studiow.be"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "lune/variables": "^0.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "psr/http-message": "^1.0",
    "guzzlehttp/psr7": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Lune\\Template\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lune\\Template\\Tests\\": "tests"
    }
  },
  "suggest":{
    "psr/http-message": "Use a psr-7 compatible library to render Http responses"
  }
}
